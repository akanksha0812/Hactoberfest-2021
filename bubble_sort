#include<iostream>
using namespace std;
int main ()
{
   int i, j,temp;
   int a[10] = {10,2,0,14,43,25,18,1,5,45};
   cout <<"Input list ...\n";
   for(i = 0; i<10; i++) {
      cout <<a[i]<<"\t";
   }
int counter=1;
while(counter<10){
    for(int i=0;i<10-counter;i++){
        if (a[i]>a[i+1]){
            int temp=a[i];
            a[i]=a[i+1];
            a[i+1]=temp;
        }
     }
     counter++;
 }
cout <<"Sorted Element List ...\n";
for(i = 0; i<10; i++) {
   cout <<a[i]<<"\t";
}
return 0;
}
